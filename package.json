{
  "name": "react-proj",
  "version": "0.0.0",
  "description": "Isomorphic React Project Base",
  "main": "app.js",
  "scripts": {
    "watch": "watchify client/app.js -d -o public/bundle.js",
    "browserify": "browserify client/app.js -d -o public/bundle.js",
    "build": "npm run browserify",
    "build-prod": "browserify client/app.js | uglifyjs > public/bundle.js",
    "start": "nodemon server/server",
    "lint": "jscs . && jshint .",
    "jshint": "jshint .",
    "jscs": "jscs .",
    "test": "npm run test-unit && npm run test-functional",
    "test-unit": "mocha test/unit/*.spec.js",
    "test-functional": "mocha test/functional/*.spec.js"
  },
  "author": "Tingzhen Ming",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/mingtingzhen/react-proj-base/"
  },
  "homepage": "https://bitbucket.org/mingtingzhen/react-proj-base/",
  "dependencies": {
    "express": "~4.9.7",
    "express-handlebars": "~1.1.0",
    "mongoose": "^3.8.23",
    "mysql": "^2.7.0",
    "node-jsx": "~0.11.0",
    "react": "~0.11.2",
    "sequelize": "^3.1.1",
    "socket.io": "^1.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^7.0.2",
    "expect.js": "^0.3.1",
    "jscs": "^1.13.1",
    "jshint": "^2.7.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.2.1",
    "precommit-hook": "^2.0.1",
    "reactify": "~0.14.0",
    "sassify": "0.5.2",
    "sqlite3": "^3.0.8",
    "uglify-js": "~2.4.15",
    "watchify": "2.2.1"
  },
  "browserify": {
    "transform": [
      "sassify",
      "reactify"
    ]
  }
}
