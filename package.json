{
  "name": "react-proj",
  "version": "0.0.0",
  "description": "Isomorphic React Project Base",
  "main": "app.js",
  "scripts": {
    "watch": "watchify client/app.js -d -o dist/bundle.js",
    "browserify": "browserify client/app.js -d -o dist/bundle.js",
