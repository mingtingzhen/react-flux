{
  "name": "react-proj",
  "version": "0.0.0",
  "description": "Isomorphic React Project Base",
  "main": "app.js",
  "scripts": {
    "watch": "watchify client/app.jsx -d -o public/bundle.js",
    "build": "browserify client/app.jsx -d -o public/bundle.js",
    "build-prod": "browserify client/app.jsx | uglifyjs > public/bundle.js",
    "start": "nodemon server/server",
    "postinstall": "npm run build",
    "lint": "jscs . && jsxhint .",
    "jshint": "jsxhint .",
    "jscs": "jscs .",
    "test": "jest"
  },
  "author": "Tingzhen Ming",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/mingtingzhen/react-proj-base/"
  },
  "homepage": "https://bitbucket.org/mingtingzhen/react-proj-base/",
  "dependencies": {
    "express": "~4.9.7",
    "express-handlebars": "~1.1.0",
    "mongoose": "^3.8.23",
    "mysql": "^2.7.0",
    "node-jsx": "^0.13.3",
    "react": "^0.13.3",
    "react-tools": "^0.13.3",
    "sequelize": "^3.1.1",
    "socket.io": "^1.1.0",
    "underscore": "^1.8.3",
    "browserify": "^10.2.3",
    "nodemon": "^1.2.1",
    "reactify": "^1.1.1",
    "sassify": "0.5.2",
    "sqlite3": "^3.0.8",
    "watchify": "2.2.1"
  },
  "devDependencies": {
    "esprima-fb": "^15001.1.0-dev-harmony-fb",
    "jest-cli": "^0.4.5",
    "jscs": "^1.13.1",
    "jshint": "^2.7.0",
    "jsxhint": "^0.15.1",
    "precommit-hook": "^2.0.1",
    "uglify-js": "~2.4.15"
  },
  "browserify": {
    "transform": [
      "sassify",
      "reactify"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "engines": {
    "node": "0.10.x"
  }
}
